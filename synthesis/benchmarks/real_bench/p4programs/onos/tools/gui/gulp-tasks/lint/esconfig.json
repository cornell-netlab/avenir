{
    "extends": "google",
    "globals": {
        "angular": true,
        "d3": true,
        "_": true
    },
    "rules": {
        "prefer-spread": 0,
        "prefer-rest-params": 0,
        "no-var": 0,
        "comma-dangle": 1,
        "brace-style": 0,
        "no-void": 0,
        "require-jsdoc": 0,
        "padded-blocks": 0,
        "quote-props": 0,
        "no-warning-comments": 0,
        "object-curly-spacing": ["error", "always"],
        "indent": 0,
        "one-var": 0,
        "block-spacing": ["error", "always"],
        "space-before-function-paren": ["error", { "anonymous": "always", "named": "never" }],
        "max-len": ["error", 120],
        "no-invalid-this": 0
    }
}
