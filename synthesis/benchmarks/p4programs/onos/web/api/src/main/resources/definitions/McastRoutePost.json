{
  "type": "object",
  "title": "route",
  "required": [
    "source",
    "group"
  ],
  "optional": [
    "ingress"
  ],
  "properties": {
    "source": {
      "type": "string",
      "example": "10.1.1.0",
      "description": "Multicast source IP address"
    },
    "group": {
      "type": "string",
      "example": "10.1.1.0",
      "description": "Multicast group IP address"
    },
    "ingress": {
      "type": "string",
      "example": "of:0000000000000001/1",
      "description": "Multicast ingress Connection Point device and port"
    }
  }
}
